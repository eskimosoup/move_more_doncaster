<div class="primary-header-wrap">
  <header class="primary-header">
    <%= link_to root_url, class: 'logo', title: global_site_settings['Name'] do %>
      <%= global_site_settings['Name'] %>
    <% end %>

    <nav class="primary-navigation">
      <ul class="primary-navigation-links">
        <%= nested_menu_items(menu_items: @header_menu.menu_items) %>
      </ul>
    </nav>

    <%= link_to 'http://www.southwestyorkshire.nhs.uk/', class: 'swyt', target: '_blank' do %>
      South West Yorkshire Partnership NHS Foundation Trust
    <% end %>
  </header>
</div>
